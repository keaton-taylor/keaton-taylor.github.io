{
  "name": "keaton-taylor.github.io",
  "version": "1.0.0",
  "main": "postcss.config.js",
  "scripts": {
    "setup:hooks": "node scripts/setup-hooks.js",
    "version:bump": "node scripts/bump-manasell-version.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "postcss assets/css/styles.css -o assets/css/main.css",
    "watch:css": "postcss assets/css/styles.css -o assets/css/main.css --watch",
    "build": "npm run build:css && bundle exec jekyll build",
    "start": "npm run watch:css & bundle exec jekyll serve --port 5000 --livereload-port 35729",
    "dev": "npm run watch:css & bundle exec jekyll serve --port 4000 --livereload-port 35729",
    "sync": "browser-sync start --config bs-config.js",
    "dev:sync": "concurrently -n \"css,jekyll,browsersync\" -c \"yellow,blue,green\" \"npm run watch:css\" \"bundle exec jekyll serve --port 4000\" \"sleep 5 && browser-sync start --config bs-config.js\""
  },
  "keywords": [
    "portfolio",
    "jekyll",
    "tailwindcss",
    "design",
    "product-design"
  ],
  "author": "Keaton Taylor",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "browserslist": "^4.24.4",
    "caniuse-lite": "^1.0.30001707",
    "cssesc": "^3.0.0",
    "electron-to-chromium": "^1.5.128",
    "escalade": "^3.2.0",
    "fraction.js": "^4.3.7",
    "lodash.castarray": "^4.4.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.merge": "^4.6.2",
    "mini-svg-data-uri": "^1.4.4",
    "nanoid": "^3.3.11",
    "node-releases": "^2.0.19",
    "normalize-range": "^0.1.2",
    "picocolors": "^1.1.1",
    "postcss-selector-parser": "^6.0.10",
    "postcss-value-parser": "^4.2.0",
    "source-map-js": "^1.2.1",
    "tailwindcss": "^3.4.1",
    "update-browserslist-db": "^1.1.3",
    "util-deprecate": "^1.0.2"
  },
  "description": "Personal portfolio website for Keaton Taylor - Product Design Leader"
}
